  popMatrix();
  
  pushMatrix();
  fill(255,255,0);
  stroke(255,255,0);
  translate(600,-10);
  scale(10);
  ellipse(10,10,10,10);
  popMatrix();
  
  pushMatrix();
  fill(255);
  stroke(255);
  translate(200,15);
  scale(5);
  ellipse(10,10,30,10);
   translate(-50,15);
  ellipse(10,10,30,10);
  translate(200,-15);
  ellipse(10,10,30,10);
  popMatrix();
 
  pushMatrix();
  translate(210,320); 
  scale(1.3);
  Jugador1.monos();
  popMatrix();
  
  
   pushMatrix();
  translate(750,320); 
  scale(1.3);
  Jugador2.monos();
  popMatrix();
  println(barra1);
  println(barra2);
  fill(0);
  text(barra2,300,300);
  text(barra1,800,300);
  
   if(turno == 1){
  if (keyPressed){
    if (key == 'f'){
      Jugador1.pv -= Jugador2.ataque; 
      barra1 = Jugador1.pv;
      turno += 1;
    }
  }
}
 if(turno == 2){
  if (keyPressed){
    if (key == 'j'){
      Jugador2.pv -= Jugador1.ataque; 
       barra2 = Jugador2.pv;
      turno -= 1;
    }
  }
}
if(Jugador1.pv <=0){
  ganador = 2;
  pantalla += 1;
}

if(Jugador2.pv <=0){
  ganador = 1;
  pantalla += 1;
  
}
}
