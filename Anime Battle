void pGanador(){
  background(255,0,0);
  if(ganador == 1){
    fill(255);
    text("Jugador2 Gano",width/2,height/2);
  }
  else{
    fill(255);
    text("Jugador1 Gano",width/2,height/2);
  }
   
    if(keyPressed){
      if(key == ENTER){
        pantalla = 0;
        turno = 1;
        barra1 = 50;
        barra2 =50;
        Jugador1.pv = 50;
        Jugador2.pv = 50;
        
      }
    }
  
}

class Anime{
  String nombre;
  int pv;
  int ataque;
  int dibujo;
  
  Anime(String nombre_,int pv_, int ataque_, int dibujo_){
    nombre = nombre_;
    pv = pv_;
    ataque= ataque_;
    dibujo = dibujo_;
  }
  
  void monos(){
  switch(dibujo){
    case 0: // Mikasa
    fill(0,0,0); //Negro
  noStroke(); 
