  rect (75,75,5,5);rect (85,75,5,5);rect (90,75,5,5);rect (95,75,5,5);rect (105,75,5,5);rect (110,75,5,5);rect (75,80,5,5);rect (80,80,5,5);rect (85,80,5,5);rect (90,80,5,5);rect (95,80,5,5);rect (100,80,5,5);rect (105,80,5,5);rect (90,90,5,5);rect (80,95,5,5);
  rect (100,95,5,5);rect (85,115,5,5);rect (95,115,5,5);
  
  //Morado
  noStroke();
  fill (140,86,138);
  rect (80,70,5,5);rect (100,70,5,5);rect (85,95,5,5);rect (95,95,5,5);rect (85,100,5,5);rect (95,100,5,5);
  
  //Amarillo
  noStroke ();
  fill (250,210,001);
  rect (85,90,5,5);rect (95,90,5,5);rect (75,100,5,5);rect (105,100,5,5);rect (85,105,5,5);rect (90,105,5,5);rect (95,105,5,5);rect (80,110,5,5);rect (85,110,5,5);rect (90,110,5,5);rect (95,110,5,5);rect (100,110,5,5);rect (85,120,5,5);rect (95,120,5,5);rect (90,65,5,5);
  rect (90,95,5,5);
  
  //Rayos
  noStroke ();
  fill (143,139,102);
  rect (65,40,5,5);rect (115,40,5,5);rect (65,45,5,5);rect (70,45,5,5);rect (110,45,5,5);rect (115,45,5,5);rect (65,50,5,5);rect (70,50,5,5);rect (75,50,5,5);rect (105,50,5,5);rect (110,50,5,5);rect (115,50,5,5);
    break;
  }
}
  
}


