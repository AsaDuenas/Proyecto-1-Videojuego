//Variables Globales
import processing.sound.*;

SoundFile soundfile;
int pantalla = 0;
int turno =1;
int barra1 = 50;
int barra2 = 50;
int ganador;


Anime Shingeki;
Anime Sailor;
Anime Sword;
Anime Goku;
Anime Dark;

Anime Shingeki2;
Anime Sailor2;
Anime Sword2;
Anime Goku2;
Anime Dark2;

Anime Jugador1;
Anime Jugador2;


void setup(){
  size(1180,660);
  Shingeki = new Anime("Mikasa",50,15,0);
  Sailor = new Anime("Sailor Moon",50,10,1);
  Sword = new Anime("Azuna",50,15,2);
  Goku = new Anime("Goku",50,5,3);
  Dark = new Anime("Black Lady",50,20,4);
  
  
  Shingeki2 = new Anime("Mikasa",50,15,0);
  Sailor2 = new Anime("Sailor Moon",50,10,1);
  Sword2 = new Anime("Azuna",50,15,2);
  Goku2 = new Anime("Goku",50,5,3);
  Dark2 = new Anime("Black Lady",50,20,4);

  soundfile = new SoundFile(this, "RedVelvetDumbdumb8bits.mp3");
  soundfile.play();
 // soundfile.amp(5);
}

void draw(){
  switch(pantalla){
    case 0: //Inicio
    pInicio();

    break;
    case 1://Pantalla de Seleccion
    pSeleccion1();
    break;
    case 2://Pantalla de Seleccion 2
    pSeleccion2();
    break;
    case 3://Pantalla de Pelea
    pPelea();
    break;
    case 4://Pantalla del ganador 
    pGanador();
    break;
    
  }
}



void pInicio(){
  background(0);
  textSize(150);
  fill(255);
  text("Anime Battle",100,300);
  textSize(70);
  text("Haz Clic",450,600);
  if (mousePressed == true){
    pantalla +=1;
  }  
}

