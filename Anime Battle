         if (keyPressed){
    
    if (key == 'm' || key == 'M'){
     Jugador1 = Shingeki;
       pantalla +=1;
      } 
      

     if (key == 'u' || key == 'U'){
     Jugador1 = Sailor;
        pantalla +=1;
      }
      
      if (key == 'a' || key == 'A'){
     Jugador1 = Sword;
        pantalla +=1;
      }
      
      if (key == 'g' || key == 'G'){
     Jugador1 = Goku;
        pantalla +=1;
      }
      
     if (key == 'b' || key == 'B'){
     Jugador1 = Dark;
       pantalla +=1;
      }
   
   }
}    

