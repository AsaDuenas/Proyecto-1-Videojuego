popMatrix();

  pushMatrix();
  fill(255);
stroke(255);
translate(1,130);
  scale(10);
  triangle(10,20,30,20,20,10);
  popMatrix();
  
    pushMatrix();
  fill(255);
stroke(255);
translate(250,130);
  scale(10);
  triangle(10,20,30,20,20,10);
  popMatrix();
  
      pushMatrix();
  fill(255);
stroke(255);
translate(750,130);
  scale(10);
