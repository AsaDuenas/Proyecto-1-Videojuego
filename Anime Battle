     Jugador2 = Shingeki2;
       pantalla +=1;
      } 
      

     if (key == 's' || key == 'S'){
     Jugador2 = Sailor2;
        pantalla +=1;
      }
      
      if (key == 'z' || key == 'Z'){
     Jugador2 = Sword2;
        pantalla +=1;
      }
      
      if (key == 'o' || key == 'O'){
     Jugador2 = Goku2;
        pantalla +=1;
      }
      
     if (key == 'l' || key == 'L'){
     Jugador2 = Dark2;
       pantalla +=1;
      }
   
   }
